    },
    "autoload": {
        "psr-0": {
            "Tecspro\\Bundle\\ComunBundle": ""
        }
    },
    "target-dir": "Tecspro/Bundle/ComunBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
