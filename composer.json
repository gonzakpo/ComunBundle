    "keywords": ["comun", "soluciones", "circunstancias", "Symfony"],
    "homepage": "https://github.com/gonzakpo/ComunBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Gonzalo Alonso",
            "email": "gonkpo@gmail.com"
        }
    ],
    "require": {
        "symfony/symfony": ">2.3.*",
